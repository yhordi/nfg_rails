<div class ="log_in">
  <div class="error"><%= flash[:notice] if flash[:notice] %></div>
    <%= form_tag sessions_path do %>
      <%= submit_tag "Log In" %>
      <%= text_field_tag :username, nil, :placeholder => "Username"  %>
      <%= password_field_tag :password, nil, :placeholder => "Password"  %>
    <% end %>
  <p class="sign_up"></p>
</div>