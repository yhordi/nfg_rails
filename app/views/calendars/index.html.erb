<div>
  <h1 class="neb">Upcoming Shows</h2>
  <% @calendar.each do |item| %>
  <ul class="events">
    <li>
      <h2 class="neb"><%= item[:summary] %></h2>
    </li>
    <ul class="events">
      <li>
        <%= item[:location] %>
      </li>
      <li>
        <%= item[:readable_time] %>
      </li>
    </ul>
  </ul>
  <% end %>
</div>
<% if session[:id] != nil && Calendar.first == nil %>
  <%= form_for Calendar.new do |f| %>
  <%= f.submit "create" %>
  <% end %>
<% end %>